# Environment Variables
NODE_ENV=development
PORT=5000

# Database
MONGO_URI=mongodb://localhost:27017/streetpaws

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRE=7d

# Google GenAI
GOOGLE_GENAI_API_KEY=your_google_genai_api_key_here

# Email (for notifications)
